{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": "off",
      "react/jsx-key": "off",
      "react/react-in-jsx-scope": "off",
      "no-empty-pattern": "off",
      "no-unused-vars": "warn",
      "no-console": "warn"
    //   "quotes": ["error", "single"]
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  }